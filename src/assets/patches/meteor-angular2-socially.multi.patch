+  collection: Images.collection,
+  name: 'images',
+  filter: new UploadFS.Filter({
+    contentTypes: ['image/*']
+  }),
+  copyTo: [
+    ThumbsStore
+  ],
+  permissions: new UploadFS.StorePermissions({
+    insert: loggedIn,
+    update: loggedIn,
+    remove: loggedIn
+  })
+});
